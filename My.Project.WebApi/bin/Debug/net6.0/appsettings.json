{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "messagebus": {
    "Brokers": "localhost:9093,localhost:9094,localhost:9095"
  },
  "Cache": {
    "CacheType": "Redis",
    "RedisEndpoint": "127.0.0.1:6379,password=123,defaultDatabase=13,prefix=my_", //单机
    "ClusterRedisEndpoint": "127.0.0.1:6379,password=123,defaultDatabase=0,poolsize=50,prefix=", //这里使用集群
    "UpdateOrderCacheOnServerRestart": true,
    "MaxDayOfHistoryOrder": 7
  },
  "distributedLock": {
    "LockTypes": "Redis",
    "RedisEndPoints": [ "lcoalhost:6379,password=123" ] 
  },
  "AllowedHosts": "*"
}
